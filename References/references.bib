issn = {03064379},
journal = {Information Systems},
keywords = {Business intelligence,Business process management,Performance analysis,Process mining,Time prediction,[04]},
mendeley-groups = {Revisi{\'{o}}n Bibliogr{\'{a}}fica},
mendeley-tags = {[04]},
